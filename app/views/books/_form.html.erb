<%= form_for @book do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>

  <%= f.label :author %>
  <%= f.text_field :author %>

  <%= f.label :publisher %>
  <%= f.text_field :publisher %>

  <%= f.label :genre %>
  <%= f.text_field :genre %>
  <%= f.submit %>
<% end %>
